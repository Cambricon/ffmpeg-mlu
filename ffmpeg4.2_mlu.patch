+static inline int mlumpp_adapt_resolution(AVCodecContext* avctx) {
+    if (avctx->codec->id == AV_CODEC_ID_MJPEG) {
+        if (avctx->width < 64 ||  avctx->width > 8192 || avctx->height < 64 || avctx->height > 4320) {
