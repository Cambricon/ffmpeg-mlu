+        input.streamBuffer = NULL;
+        input.streamLength = 0;
+        input.pts = 0;
+        input.flags |= CNJPEGDEC_FLAG_EOS;
+        if (ctx->trace_flag > 0) {
+            av_log(avctx, AV_LOG_INFO, "cndecoder jpeg feed eos ... \n");
