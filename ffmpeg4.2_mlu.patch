+        CNCODEC_ERROR_CHECK(avctx,ret);
+        if (ctx->trace_flag > 1) {
+            av_log(avctx, AV_LOG_INFO, "Feed stream info: EOS, flags: %u, pts: (%3" PRId64 ")\n", input.flags, input.pts);
+        }
+    }
